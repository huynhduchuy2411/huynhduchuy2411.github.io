<script src="https://cdn.jsdelivr.net/gh/stevenschobert/instafeed.js@2.0.0rc1/src/instafeed.min.js"></script>
<!-- <script src="https://cdnjs.cloudflare.com/ajax/libs/instafeed.js/1.4.1/instafeed.min.js"></script> -->
<script type="text/javascript">
$(document).ready(function () {

  var userFeed = new Instafeed({
    get: 'user',
    target: "instafeed",
    accessToken: 'IGQVJWZAXVsSG9ERVB5Y1dtYmFRT3I0elRyd2dIVkJ0eEFyTnRnLVB1b1J6VlFsem1LUUowbkdiQjRTcWFyRFpwczhWaFVvQnB2VTVWY01XNFZAuX1g4MHB4aGlnVGl2RUZArS0t2bl9ld2RwQzlEbVlfMwZDZD',
    resolution: 'low_resolution',
    sortBy: 'most-recent',
    template:'<div class="instaitem " style="margin-right: 0px; margin-left: 0px;"><a href={{link}} target="_blank" id={{id}}><img class="lazyload" data-src={{image}} /><div class="image-overlay"><span></span></div><span class="instagram"><i class="fa fa-instagram" aria-hidden="true"></i></span></a><div class="text-container"><span class="likes"><span class="likes-count">{{likes}}</span><i class="fa fa-heart" aria-hidden="true"></i></span><span class="comments"><span class="comments-count">{{comments}}</span><i class="fa fa-comments" aria-hidden="true"></i></span></div></div>',
    links: false,
    after: function(){
        $('.instafeed').owlCarousel({
          nav: false,
          loop: true,
          responsive: {
            0: {
              items: 1
            },
            376: {
              items: 2
            },
            768: {
              items: 3
            },
            992: {
              items: 4
            },
            1200: {
              items: 6
            }
          }
        });
        insertDummyData();
    }
  });
  userFeed.run();
});

function insertDummyData() {

    $(".instafeed .owl-item").each(function(){
      $(this).find(".comments-count").text(niceNumberDisplay(Math.floor((Math.random() * 10000) + 1)));
      $(this).find(".likes-count").text(niceNumberDisplay(Math.floor((Math.random() * 100000) + 1)));
    });
}



function niceNumberDisplay(num) {
    var n = parseFloat(num);
    if(n > 1000000) {
        return Math.round(n / 1000000) + 'm';
    } else if(n > 1000) {
        return Math.round(n / 1000) + 'k';
    } else {
        return n.toFixed(0);
    }
}
</script>



