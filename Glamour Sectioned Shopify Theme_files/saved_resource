     <script type="text/javascript">
     $(document).ready(function () {
        $(".ishi-singleproduct-section .product-tab .custom-tab").each(function(index) {
          $('.ishi-singleproduct-section .nav-tabs').append('<li class="tab-item"> <a href="#tab'+ index +'" class="nav-link" data-toggle="tab" role="tab">'+ $(this).find('.title').html() +'</a> </li>');
          $('.ishi-singleproduct-section #tab-content').append('<div id="tab'+ index +'" class="custom-desc tab-pane fade in">'+ $(this).find('.description').html() +'</div>');
          $(this).remove();
        });
      });
      </script>
    
    
